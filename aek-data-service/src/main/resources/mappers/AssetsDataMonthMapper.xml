<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aek.ebey.data.mapper.AssetsDataMonthMapper">

	<!-- 通用查询映射结果 -->
	<resultMap id="BaseResultMap" type="com.aek.ebey.data.model.AssetsDataMonth">
		<id column="id" property="id" />
		<result column="tenant_id" property="tenantId" />
		<result column="count_month" property="countMonth" />
		<result column="assets_total_num" property="assetsTotalNum" />
		<result column="assets_total_capital" property="assetsTotalCapital" />
		<result column="count_time" property="countTime" />
		<result column="update_time" property="updateTime" />
	</resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, tenant_id AS tenantId, count_month AS countMonth, assets_total_num AS assetsTotalNum, assets_total_capital AS assetsTotalCapital, count_time AS countTime, update_time AS updateTime
    </sql>
</mapper>
